vol_down=mp.command("add volume -1")
vol_mute=mp.command("cycle mute")
toggle_drc=mp.command("af toggle drc")
vol_up=mp.command("add volume 1")
pl_prev=mp.command("playlist_prev")
ch_prev=mp.command("add chapter -1")
ch_next=mp.command("add chapter 1")
pl_next=mp.command("playlist_next")
rev_long=mp.command("seek -300")
rev_short=mp.command("seek -10")
playpause=mp.command("cycle pause")
fw_short=mp.command("seek 10")
fw_long=mp.command("seek 300")
subdelay_minus=mp.command("add sub-delay -0.5")
sub=mp.command("cycle sub")
subdelay_plus=mp.command("add sub-delay +0.5")
audio=mp.command("cycle audio")
stop=mp.command("stop")
info=file=display_info.lua
fs=mp.command("cycle fullscreen")
