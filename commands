vol_set=set volume {}
toggle_drc=af toggle drc
pl_prev=playlist_prev
ch_prev=add chapter -1
ch_next=add chapter 1
pl_next=playlist_next
rev_long=seek -60
rev_short=seek -5
playpause=cycle pause
fw_short=seek 5
fw_long=seek 60
subdelay_minus=add sub-delay -0.1
sub=cycle sub
subdelay_plus=add sub-delay +0.1
audio=cycle audio
stop=quit
info=show_progress
fs=cycle fullscreen
