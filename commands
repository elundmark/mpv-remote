vol_down=add volume -1
vol_mute=cycle mute
toggle_drc=af toggle drc
vol_up=add volume 1
pl_prev=playlist_prev
ch_prev=add chapter -1
ch_next=add chapter 1
pl_next=playlist_next
rev_long=seek -300
rev_short=seek -10
playpause=cycle pause
fw_short=seek 10
fw_long=seek 300
subdelay_minus=add sub-delay -0.5
sub=cycle sub
subdelay_plus=add sub-delay +0.5
audio=cycle audio
stop=quit
info=show_progress
fs=cycle fullscreen
