vol_set=set volume {}
toggle_drc=af toggle drc
ch_prev=add chapter -1
ch_next=add chapter 1
pl_prev=playlist_prev
pl_next=playlist_next
playpause=cycle pause
seek=seek {}
sub=cycle sub
subdelay=add sub-delay {}
audio=cycle audio
audiodelay=add audio-delay {}
stop=quit
info=show_progress
fs=cycle fullscreen
subdl=subdl
rescan=rescan_external_files reselect
message=show_text "{}"
